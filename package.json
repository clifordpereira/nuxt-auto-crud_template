{
  "name": "auto-crud.clifland.in",
  "private": true,
  "type": "module",
  "scripts": {
    "// --- Development ---": "",
    "dev": "nuxi dev",
    "postinstall": "nuxi prepare",
    "preview": "nuxi preview",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck",
    "// --- Database (NuxtHub/Drizzle) ---": "",
    "db:generate": "nuxi db generate",
    "db:migrate": "nuxi db migrate",
    "db:studio": "nuxi db studio",
    "// --- Build ---": "",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' nuxi build",
    "// --- Prod ---": "",
    "migrate:prod": "nuxi db migrate --remote",
    "deploy:prod": "bun run build && bun run migrate:prod && wrangler deploy",
    "logs:prod": "wrangler tail",
    "// --- Staging ---": "",
    "migrate:staging": "nuxi db migrate --remote --env staging",
    "deploy:staging": "bun run build && bun run migrate:staging && wrangler deploy --env staging",
    "logs:staging": "wrangler tail --env staging"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.86",
    "@iconify-json/simple-icons": "^1.2.67",
    "@nuxt/content": "^3.11.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.4.0",
    "@nuxthub/core": "^0.10.5",
    "@standard-schema/spec": "^1.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "bcryptjs": "^3.0.3",
    "drizzle-orm": "^0.45.1",
    "nuxt": "^4.3.0",
    "nuxt-auth-utils": "^0.5.27",
    "nuxt-authorization": "^0.3.5",
    "nuxt-auto-crud": "^1.31.0",
    "nuxt-delay-hydration": "^1.3.8",
    "nuxt-security": "^2.5.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/vscode-icons": "^1.2.40",
    "@libsql/client": "^0.17.0",
    "@nuxt/eslint": "^1.13.0",
    "@nuxt/fonts": "^0.13.0",
    "@nuxt/scripts": "^0.13.2",
    "@types/bcryptjs": "^3.0.0",
    "better-sqlite3": "^12.6.2",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "happy-dom": "^20.3.9",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.2.3",
    "wrangler": "^4.60.0",
    "xlsx": "^0.18.5"
  },
  "packageManager": "bun@1.2.5"
}